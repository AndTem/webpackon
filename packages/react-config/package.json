{
  "name": "@webpackon/react-config",
  "version": "1.2.4",
  "description": "Webpack 4 config for SPA",
  "author": "And_tem",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndTem/webpack-spa-config"
  },
  "license": "MIT",
  "homepage": "https://github.com/AndTem/webpack-spa-config#Readme",
  "keywords": [
    "webpack",
    "webpack 5",
    "webpack config",
    "webpack-config",
    "webpack spa",
    "webpack compatibility config",
    "webpack modern config"
  ],
  "bugs": {
    "url": "https://github.com/AndTem/webpack-spa-config/issues"
  },
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "tsc": "../../node_modules/.bin/tsc",
    "remove-build": "../../node_modules/.bin/rimraf lib/*",
    "build": "yarn run remove-build && yarn run tsc",
    "lint:fix": "eslint ./src --fix --ext .ts",
    "lint:types": "tsc --noEmit",
    "tests": "jest",
    "linting": "yarn run lint:fix && yarn run lint:types && yarn run tests",
    "release": "yarn run linting && yarn run build && npm publish"
  },
  "dependencies": {
    "@webpackon/core": "^1.0.0",
    "@webpackon/babel": "^1.0.0",
    "@webpackon/typescript": "^1.0.0",
    "@webpackon/base-config": "^1.0.0",
    "@webpackon/optimization": "^1.0.0",
    "@types/webpack": "5.28.0",
    "react-refresh": "0.10.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.1",
    "react-refresh-typescript": "2.0.2"
  },
  "peerDependencies": {
    "webpack": "5.x.x",
    "webpack-dev-server": "3.x.x"
  }
}
